export POSTGRES_USER=postgres
export POSTGRES_PASSWORD=Pass1234!
export POSTGRES_DB=dbosexp
export POSTGRES_PORT=5432
export POSTGRES_HOST=localhost
export POSTGRES_URL=postgresql://$POSTGRES_USER:$POSTGRES_PASSWORD@$POSTGRES_HOST:$POSTGRES_PORT/$POSTGRES_DB
# Set the default postgres environment variables
export PGUSER=$POSTGRES_USER
export PGPASSWORD=$POSTGRES_PASSWORD
export PGDATABASE=$POSTGRES_DB
export PGPORT=$POSTGRES_PORT
export PGHOST=$POSTGRES_HOST
